---
import Cards from "./Cards.astro";

const myProjects = [
    'project1',
    'project2',
    'project3',
    'project4',
    'project5',    
]
---
<div>    
    <p class="mx-4 mt-12 mb-6 text-2xl">My Projects</p>
    <div class="grid sm:grid-cols-6">
            {myProjects.map(myProject => <Cards topic={myProject}/>)}
    </div>
</div>